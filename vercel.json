{
    "version": 2,
    "builds": [
      {
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "s-maxage=31536000,immutable" },
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "headers": { "cache-control": "s-maxage=31536000,immutable" },
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "core/wsgi.py"
      }
    ]
  }